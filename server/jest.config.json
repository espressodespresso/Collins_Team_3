{
  "testEnvironment": "node",
  "roots": ["tests"],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.mjs$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "src/node_modules/(?!@hokify)"
  ],
  "moduleFileExtensions": [
    "js",
    "mjs"
  ],
  "moduleDirectories": ["node_modules", "src"]
}